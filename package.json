{
  "name": "@sequent-org/ifc-viewer",
  "private": false,
  "version": "1.2.3",
  "type": "module",
  "description": "IFC 3D model viewer component for web applications - fully self-contained with local IFCLoader",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "files": [
    "src/",
    "scripts/",
    "README.md"
  ],
  "keywords": [
    "ifc",
    "3d",
    "viewer",
    "bim",
    "three.js",
    "web-ifc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sequent-org/ifc-viewer.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postinstall": "node scripts/copy-web-ifc-wasm.mjs && node scripts/copy-rhino3dm.mjs && node scripts/patch-three-3dm-loader.mjs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:manual": "vite dev --open test.html"
  },
  "devDependencies": {
    "vite": "^7.1.2"
  },
  "dependencies": {
    "rhino3dm": "8.4.0",
    "three": "^0.149.0",
    "web-ifc": "^0.0.74"
  }
}
